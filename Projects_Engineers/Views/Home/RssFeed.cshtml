@using Projects_Engineers_Data;

@{
    ViewBag.Title = "Feed";
}
<br />
<h2>Honeywell Intelligrated</h2>
<hr />
<br />

<div style="height:200px;overflow:auto;">

    <table class="table table-hover">
        <tbody>
            @if (ViewBag.RSSFeed != null)
            {
                foreach (var usr in ((List<PE_User>)ViewBag.RSSFeed).Take(10).OrderBy(x => x.Name))
                {
                    <tr>
                        <td>User <b>@usr.Name</b> added new user <b>@usr.Name</b></td>
                    </tr>
                }
            }
        </tbody>
    </table>

</div>